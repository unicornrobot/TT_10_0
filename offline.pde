int numPoints = 700;

IntList generateSmoothRandomList(int numPoints, float noiseOffset) {
  IntList smoothList = new IntList();
  float xoff = 0.0;
  
  for (int i = 0; i < numPoints; i++) {
    float noiseValue = noise(xoff + noiseOffset * 10000) * 300; // Use different noise offsets
    int intValue = int(noiseValue);
    smoothList.append(intValue);
    //xoff += 0.001; // Adjust the noise increment for different smoothness levels
    //xoff += 0.05;
    xoff += 0.03;
  }
  smoothList.append(smoothList.get(0));//Add the first value at the end to close the shape
  
  return smoothList;
}


